with doc(x) as (
    select (graphql.parse_query(
        $$
            {
              accountConnection(last: 2, before: "WyJhY2NvdW50IiwgM10=") {
                edges {
                  node {
                    nodeId
                    id
                  }
                }
              }
            }
        $$
    ))
)
select
    jkr.jpath,
    jkr.obj
from
