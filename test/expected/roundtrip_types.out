                    typeTime: "18:23"
                    typeDatetime: "1900-01-01 18:00"
                    typeEnum: RED
                    typeFloat: 25.1
                    typeDouble: 25
                    typeNumeric: "25.1"
                    typeArrayEnum: [RED, RED, GREEN]
                    typeArrayText: ["a", "b"]
                    typeArrayJson: ["{\"a\": 1}"]
                    typeArrayNumeric: ["2.5"]

